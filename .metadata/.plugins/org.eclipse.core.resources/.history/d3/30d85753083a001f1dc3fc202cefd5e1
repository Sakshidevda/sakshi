package July4;

public class Student {
	
	private String id;
    private String name;
    private String address;
    
    
    public Student(String id, String name, String address) {
        this.id= id;
        this.name = name;
        this.address= address;
       
    }
    
    public String getid() {
        return id;
    }
    
    public String getname() {
        return name;
    }
    
    public String getaddress() {
        return address;
    }
    
   
	
Student[] students = new Student[5] ;


students[0] = new Student("AF04", "Sakshi Devda", "450 kotra bhopal");
students[1] = new Student("AF05", "Khushi Sharma", "456 nehru nager indor");
students[2] = new Student("AF04", "Swati srivastav", "789 kolar kanpur");

for (int i = 0; i < students.length; i++) { 
System.out.println("Student" + " " );
		
}
}
}